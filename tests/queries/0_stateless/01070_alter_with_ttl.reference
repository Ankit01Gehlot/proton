create stream default.alter_ttl\n(\n    `i` int32,\n    `s` string TTL to_date(\'2020-01-01\')\n)\nENGINE = MergeTree\nORDER BY i\nTTL to_date(\'2020-05-05\')\nSETTINGS index_granularity = 8192
create stream default.alter_ttl\n(\n    `d` date,\n    `s` string TTL d + toIntervalDay(1)\n)\nENGINE = MergeTree\nORDER BY d\nTTL d + toIntervalMonth(1)\nSETTINGS index_granularity = 8192
